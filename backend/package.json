{
  "name": "backend",
  "version": "1.0.0",
  "description": "API for Collect & Cruise",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "build:backend": "tsc",
    "build:frontend": "cd ../frontend && npm install && npm run build",
    "build": "npm run build:backend && npm run build:frontend",
    "dev": "tsx watch src/server.ts",
    "seeder:import": "tsx seeder.ts",
    "seeder:destroy": "tsx seeder.ts -d"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.1",
    "multer": "^1.4.5-lts.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.3",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.15.6"
  }
}